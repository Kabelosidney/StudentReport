<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mlab.kabelo.studentreport">

    <application
        android:allowBackup="true"
        android:icon="@drawable/theme1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/theme1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".TeachersLoggin"
            android:label="Teachers Login"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StudentLoggin"
            android:label="Student Login"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".StudentSignupActivity"
            android:label="Student SignUp"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".TeachersSignupActivity"
            android:label="Teachers SignUp"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".TeachersAdmin"
            android:label="Admin"
            android:parentActivityName=".TeachersLoggin" />
        <activity
            android:name=".StudentReports"
            android:label="Students Report"
            android:parentActivityName=".StudentLoggin"
            />
    </application>

</manifest>